<table class="purchase-table table">
    <thead>
        <th>ID</th>
        <th>Amount</th>
        <th>Category</th>
        <th>Description</th>
        <th>Purchase Date</th>
        <th>Vendor</th>
    </thead>
    <tbody id="purchase-table-items">
        {{#each model.purchases as |purchase|}}
        <tr>
            <td>
                {{purchase.id}}
            </td>
            <td>
                {{purchase.amount}}
            </td>
            <td>
                {{purchase.category}}
            </td>
            <td>
                {{purchase.description}}
            </td>
            <td>
                {{purchase.purchase_date}}
            </td>
            <td>
                {{purchase.vendor_id}}
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>